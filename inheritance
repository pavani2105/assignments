class A():
    def display(dp):
        print("inside class A")
    def show(self):
        print("show in class A")
class B(A):
    def print(pt):
        print("inside class B")
    def show(self):
        print("show in Class B")
class C(B):
    def show(self):
        print("show in class C")
s=A()
s.display()
k=B()
k.print()
g=C()
g.show()
