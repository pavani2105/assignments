class Student:
    def __init__(self,name,age):
        self.name=name
        self.age=age
    def display(self):
        print(self.name)
        print(self.age)
class Person(Student):
    def __init__(self,name,age,roll):
        super().__init__(name,age)
        self.roll=roll
    def display(self):
        print(self.name)
        print(self.roll)
s=Person("a",20,1)
s.display()
